#include <stdio.h>

int main() {
    char ch;
    printf("enter a character:");
    scanf("%c", &ch);
    
    if(ch >= 'A' && ch<='Z'){
        printf("%c is an uppercase alphabet.",ch);
    } else if(ch>='a' && ch<='z') {
        printf("%c is an lowercase alphabet.",ch);
    } else {
        printf("%c is not an alphabet.", ch);
    }
    return 0;
}
